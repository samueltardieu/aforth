with "areadline";

project Aforth is

  for Main use ("test_aforth");

  package Builder is
    for Default_Switches ("Ada") use ("-g");
    for Global_Configuration_Pragmas use "gnat.adc";
  end Builder;

  package Compiler is
    for Default_Switches ("Ada") use ("-g", "-O2", "-gnatg", "-gnaty", "-gnatwa");
  end Compiler;

end Aforth;
